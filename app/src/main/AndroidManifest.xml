<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ba95m.sendme">

    <uses-permission android:name="android.permission.INTERNET" />

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/staff"
        android:label="@string/app_name"
        android:roundIcon="@drawable/staff"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ChoiceActivity" />
        <activity android:name=".activities.SignupActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.ProfileActivity" />
        <activity android:name=".activities.AnimeActivity" />
        <activity android:name=".activities.ContactActivity" />
        <activity android:name=".activities.NewContratActivity" />
        <activity android:name=".activities.NewExpActivity" />
        <activity android:name=".activities.CompleteProfileActivity" />
        <activity android:name=".activities.StartingMissionActivity" />
        <activity android:name=".activities.PaymentActivity"></activity>
    </application>

</manifest>